<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
  <style name="ActionBarElfecTheme" parent="android:Widget.ActionBar">
      <item name="android:background">#215b92</item>
      <item name="android:textColorPrimary">@color/white</item>
  </style>
  <style name="EditTextElfecTheme" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/elfectheme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="AutoCompleteTextViewElfecTheme" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/elfectheme_list_selector_holo_light</item>
	  <item name="android:background">@drawable/elfectheme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxElfecTheme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/elfectheme_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonElfecTheme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/elfectheme_btn_radio_holo_light</item>
  </style>

  <style name="ButtonElfecTheme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/elfectheme_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="ImageButtonElfecTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/elfectheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerElfecTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/elfectheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/elfectheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemElfecTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/elfectheme_btn_radio_holo_light</item>
  </style>
  <style name="TabElfecTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/elfectheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextElfecTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarElfecTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/elfectheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/elfectheme_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarElfecTheme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/elfectheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/elfectheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/elfectheme_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarElfecTheme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/elfectheme_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/elfectheme_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigElfecTheme">
      <item name="android:progressDrawable">@drawable/elfectheme_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/elfectheme_ratingbar_holo_light</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmallElfecTheme">
      <item name="android:progressDrawable">@drawable/elfectheme_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/elfectheme_ratingbar_small_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="ToggleElfecTheme" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/elfectheme_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ListViewElfecTheme" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/elfectheme_list_selector_holo_light</item>
  </style>

  <style name="ListViewElfecTheme.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/elfectheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemElfecTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#000000</item>
  </style>
  
  <style name="DialogElfecTheme" parent="@style/ElfecTheme">
       <item name="android:windowFrame">@null</item>
        <item name="android:windowTitleStyle">@style/DialogWindowTitle</item>
        <item name="android:windowBackground">@android:drawable/alert_light_frame</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
  </style>
  
  <style name="DialogWindowTitle">
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/elfectheme_color</item>
        <item name="android:height">20dp</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textAppearance">@style/TextAppearanceDialogWindowTitle</item>
    </style>
  
  <style name="TextAppearanceDialogWindowTitle">
        <item name="android:textSize">20sp</item>
    </style>
</resources>